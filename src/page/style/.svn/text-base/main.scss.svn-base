body,ol,ul,h1,h2,h3,h4,h5,h6,p,th,td,dl,dd,form,fieldset,legend,input,textarea,select,button{margin:0 !important;padding:0}
body{font:12px"宋体","Arial Narrow",HELVETICA;background:#fff;-webkit-text-size-adjust:100%;}
a{ text-decoration:none}
em{font-style:normal}
ul{list-style:none}
li{list-style:none}
img,button{border:0;vertical-align:middle;outline: none}
button{background: #fff;cursor: pointer}
input{ outline: none;text-indent: 10px}
input:focus{
  border: 1px solid #00C9A1;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
  color: #666;
}
input:-moz-placeholder, textarea:-moz-placeholder {
  color: #666;
}
input::-moz-placeholder, textarea::-moz-placeholder {
  color: #666;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
  color: #666;
}
table{border-collapse:collapse;border-spacing:0}
p{word-wrap:break-word}
/*零 浮动*/
.clearfix:after{
  display: block;
  content: ".";
  height: 0;
  line-height: 0;
  visibility: hidden;
  clear: both;
}
.clearfix{
  zoom:1;
}
/*左浮动*/
.fl{
  float: left;
}
/*右浮动*/
.fr{
  float: right;
}
.common_banner{
  margin-top: 129px;
}
//按钮
.last{
  margin-left: 116px;
  margin-top: 40px;
  button{
    width: 120px;
    height: 47px;
    background: #00C9A1;
    border-radius: 3px;
    color: white;
  }
  button:nth-child(2){
    background: #000;
    opacity: .2;
    margin-left: 20px !important;
  }
}
//蒙层
.bgs{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 99999;
  top: 0;
  left: 0;
  background: #000000;
  opacity: .4;
}
//分页
.pages{
  width: 1200px;
  margin: auto;
  text-align: center;
  padding-bottom: 16px !important;
  .btn-prev,.btn-next{
    width: 80px;
    height: 40px;
    color: #7f7f7f !important;
    background: #fff !important;
    border: 1px solid #ccc;
  }
  .el-pager{
    li{
      margin: 0 !important;
    }
  }
 .el-pagination.is-background .el-pager li{
    background: #fff;
    width: 40px;
    height: 40px;
    line-height: 40px;
  }
  .el-pager li{
    border: 1px solid #ccc;
  }
  .el-pager li:first-child,.el-pager li:nth-child(evev){
    border-left:0
  }
  .el-pager li:first-child,.el-pager li:last-child{
    border-right:0
  }
  .el-pagination.is-background .el-pager li:not(.disabled).active{
    background: #00C9A1;
  }
}
